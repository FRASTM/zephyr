/* Connect SDA pins (D68 and D14) and SCL pins (D69 to D15) */
&i2c1 {
	eeprom0: eeprom@54 {
		compatible = "zephyr,i2c-target-eeprom";
		reg = <0x54>;
		size = <1024>;
	};
};

&i2c2 {
	eeprom1: eeprom@56 {
		compatible = "zephyr,i2c-target-eeprom";
		reg = <0x56>;
		size = <1024>;
	};
};
