&i2c1 {
	eeprom0: eeprom@54 {
		compatible = "atmel,at24";
		reg = <0x54>;
		size = <1024>;
		pagesize = <16>;
		address-width = <8>;
		timeout = <5>;
	};
};


&i2c2 {
	pinctrl-0 = <&i2c2_scl_pb10 &i2c2_sda_pb11>;
	pinctrl-names = "default";
	status = "okay";

	eeprom1: eeprom@56 {
		compatible = "atmel,at24";
		reg = <0x56>;
		size = <1024>;
		pagesize = <16>;
		address-width = <8>;
		timeout = <5>;
	};
};
